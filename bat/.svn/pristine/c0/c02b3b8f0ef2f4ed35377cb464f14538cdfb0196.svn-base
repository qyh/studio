INSERT INTO `cate` VALUES (1,'农大教育超市','暂无','农大校内',0,'为学生提供一个良好的购物天地！','超市','multiple/jiaoyuchaoshi.jpg','8：00―22：00','暂无','超市 购物'),(2,'百草味','暂无','中街',9.5,'特点：商品品种多；摆设整齐；价格实惠；会员优惠多；\r\n活动信息：①有节日时搞优惠活动；\r\n          ②搬至商业街时会换会员积分；\r\n          ③搬至商业街时会有比较大型的活动；\r\n特色商品：饼干、话梅、干果、山楂梅、金佶、软糖等\r\n','零食','multiple/baicaowei.jpg','8：00―22：00','饼干、话梅、干果、山楂梅、金佶、软糖等','零食'),(3,'美刻美食','暂无','农大生活区对面',0,'①专卖进口商品，在美刻美食可以找到自己没有吃过看过的东西。\r\n②盐津铺子系列产品、浆果、牛奶棒棒糖、酸木瓜、手工蛋片、启明星等因为好吃新颖热卖中。\r\n③特色产品有靓香芒果味软糖，为泰国进口产品。\r\n','零食','multiple/meikemeishi.jpg','8：00―22：00','盐津铺子系列产品、浆果、牛奶棒棒糖、酸木瓜、手工蛋片、启明星等','零食 美食'),(4,'四季花','暂无','农大校内',0,'品种齐全，奶茶口味清新，现调现配。\r\n特色奶茶：红枣桂圆奶茶、麦豆乌龙奶茶、蜜枣玫瑰奶醇等等\r\n','奶茶','multiple/sijihua.jpg','8：00―22：00','红枣桂圆奶茶、麦豆乌龙奶茶、蜜枣玫瑰奶醇等','奶茶'),(5,'老树音乐餐厅','暂无','紧固电厂左拐前进500米',0,'在音乐中享受美食！','餐厅 美食','multiple/laoshucanting.jpg','8：00―22：00','暂无','餐厅'),(6,'稻香楼','暂无','农大校内',0,'学生食堂，还有其他食堂。。。。。','食堂','multiple/daoxianglou.jpg','6：00-13：00  17：00―19：00','暂无','食堂'),(7,'港湾自助烤肉','暂无','江西紧固件厂老分厂',0,'自己烤肉，别样的风趣！','烧烤','multiple/shaokao.jpg','0：00―24：00','暂无','烧烤'),(8,'煌上煌','暂无','农大后街',0,'主要经营卤味菜食，烤鸭，鸭脖之类','美食','multiple/huangshanghuang.jpg','8：00―22：00','鸭脖 烤鸭之类','美食'),(9,'农场农夫烤鸡','13767098392','农大后街',0,'主要经营烤鸡类食品','美食','multiple/nongchang.jpg','8：00―22：00','暂无','美食'),(10,'轻达百货','07913846460','农大后街',0,'百货超市','超市','multiple/qingda.jpg','8：00―22：00','暂无','百货'),(11,'香酥王','13979103759','农大后街',0,'经营哈密瓜味、豆沙、凤梨、香葱、梅菜、肉松、等多种口味的饼，现烤现卖！','饼','multiple/xiangsuwang.jpg','8：00―22：00','暂无','饼'),(12,'沁人酒楼','暂无','农大生活区对面',0,'经营各种菜色','饭馆','multiple/qinren.jpg','8：00―22：00','暂无','饭馆'),(13,'萍乡酒家','13755626934','江西农大保卫处往东20米',0,'经营萍乡本地特色菜！','饭馆','multiple/pingxiang.jpg','8：00―22：00','暂无','饭馆'),(14,'川味府','13576113600','江西农大保卫处往北20米',0,'经营特色川菜！','饭馆','multiple/chuanwei.jpg','8：00―24：00','暂无','饭馆'),(15,'赣味家园','07913813659','江西农大保卫处往北20米',0,'经营特色赣菜！','饭馆','multiple/ganwei.jpg','8：00―24：00','暂无','饭馆'),(16,'学生之家餐馆','07918076821','江西农大保卫处往北20米',0,'经营各种学生常吃菜系！','美食 饭馆','multiple/xuesheng.jpg','8：00―24：00','暂无','饭馆');
